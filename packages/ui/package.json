{
  "name": "@turbo-super/ui",
  "version": "0.0.0",
  "private": true,
  "description": "Общие UI компоненты для turbo-super",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "style": "./src/ui.css",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**",
    "src/styles.css"
  ],
  "scripts": {
    "build": "node build.js",
    "build:types": "tsc -p tsconfig.build.json",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint src/",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
