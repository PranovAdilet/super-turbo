{
  "name": "@turbo-super/ai-tools",
  "version": "0.0.0",
  "private": true,
  "description": "AI Tools для генерации изображений, видео и улучшения промптов",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./client": {
      "import": "./dist/client.mjs",
      "require": "./dist/client.js",
      "types": "./dist/client.d.ts"
    }
  },
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "tsup && node restore-types.js",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint src/",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "next": "^14.0.0 || ^15.0.0"
  },
  "devDependencies": {
    "@turbo-super/eslint-config": "workspace:*",
    "@turbo-super/tsconfig": "workspace:*",
    "@types/node": "^20.17.32",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "eslint": "^8.57.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tsup": "^8.0.2",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@turbo-super/ui": "workspace:*",
    "@turbo-super/shared": "workspace:*",
    "lucide-react": "^0.446.0",
    "clsx": "^2.1.0",
    "class-variance-authority": "^0.7.0",
    "tailwind-merge": "^2.2.1"
  },
  "publishConfig": {
    "access": "restricted"
  }
} 