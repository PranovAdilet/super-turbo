  },
  "devDependencies": {
    "@turbo-super/eslint-config": "workspace:*",
    "@turbo-super/tsconfig": "workspace:*",
    "eslint": "^8.57.0",
    "tsup": "^8.0.2",
    "typescript": "^5.6.3"
  },
  "publishConfig": {
    "access": "restricted"
  }
} 